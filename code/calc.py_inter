#!/usr/bin/python
# -*- coding: utf-8 -*-
ውጤት = 0
while True :
  print("የ ሂሳብ አይነቱን ምረት : ")
  ሂሳብአይነቱ = input("->")
  print("የ መጀመርያ ቁጠሩን አስገቡ : ")
  መጀመርያቁጥር = int (input ("->"))
  print("የ ሁለተኛ ቁጠሩን አስገቡ : ")
  ሁለተኛቁጥር = int (input ("->"))
  if ሂሳብአይነቱ == '+':
    ውጤት = መጀመርያቁጥር + ሁለተኛቁጥር
  elif ሂሳብአይነቱ == '-':
    ውጤት = መጀመርያቁጥር - ሁለተኛቁጥር
  elif ሂሳብአይነቱ == '*':
    ውጤት = መጀመርያቁጥር * ሁለተኛቁጥር
  elif ሂሳብአይነቱ == '/':
    ውጤት = መጀመርያቁጥር / ሁለተኛቁጥር
  else :
    break
  print ("ውጤት:" + str (ውጤት))

